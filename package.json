{
  "name": "weniger-songs-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Songs written and song by Dr. Brad Weniger",
  "bugs": {
    "url": "https://github.com/CandeeGenerations/weniger-songs-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CandeeGenerations/weniger-songs-ui.git"
  },
  "license": "MIT",
  "author": "Candee Generations <admin@candeegenerations.com>",
  "scripts": {
    "build": "fnm use; REACT_APP_VERSION=$npm_package_version GENERATE_SOURCEMAP=false NODE_ENV=production craco build",
    "eject": "react-scripts eject",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "generate": "graphql-codegen --config codegen.yml",
    "preinstall": "npx only-allow pnpm",
    "pcheck": "npm-check -u -i 'antd' && pnpm audit",
    "prepare": "husky",
    "prettier": "prettier --cache --check --write --ignore-unknown .",
    "prettier:ci": "pnpm exec prettier --check .",
    "start": "fnm use; phase run 'REACT_APP_VERSION=$npm_package_version BROWSER=none craco start'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "diff": "^4.0.4",
    "lodash": "^4.17.23",
    "nth-check": "^2.0.1",
    "on-headers": "^1.1.0",
    "postcss": "^8.4.31",
    "qs": "^6.14.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@ant-design/icons": "6.1.0",
    "@apollo/client": "^4.1.3",
    "@craco/craco": "^7.1.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "antd": "^5.29.3",
    "copy-text-to-clipboard": "^3.2.2",
    "craco-webpack-resolve": "^1.2.1",
    "dayjs": "^1.11.19",
    "graphql": "^16.12.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.39.2",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-apollo": "^4.4.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react-app-alias": "^2.2.2",
    "sort-package-json": "^3.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
