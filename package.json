    "craco-webpack-resolve": "^1.2.1",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
